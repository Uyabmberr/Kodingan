{
  "name": "botid",
  "version": "1.5.10",
  "description": "A package with a client-side React component and a server-side API library.",
  "main": "dist/server/index.js",
  "browser": "dist/next/client/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/noop.d.ts",
      "import": "./dist/noop.mjs",
      "require": "./dist/noop.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js"
    },
    "./client/core": {
      "types": "./dist/client/core/index.d.ts",
      "import": "./dist/client/core/index.mjs",
      "require": "./dist/client/core/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js"
    },
    "./next/config": {
      "types": "./dist/next/config/index.d.ts",
      "import": "./dist/next/config/index.mjs",
      "require": "./dist/next/config/index.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt/module.d.ts",
      "import": "./dist/nuxt/module.mjs",
      "require": "./dist/nuxt/module.js"
    }
  },
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client/index.d.ts"
      ],
      "client/core": [
        "./dist/client/core/index.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ],
      "next/config": [
        "./dist/next/config/index.d.ts"
      ],
      "*": [
        "./dist/noop.d.ts"
      ]
    }
  },
  "peerDependencies": {
    "next": "*",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.27.12",
    "@nuxt/schema": "^4.0.1",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.25.6",
    "jsdom": "^26.1.0",
    "next": "^14.0.0",
    "tsup": "^8.5.0",
    "typescript": "5.6.3",
    "ultracite": "4.2.6",
    "vitest": "3.1.2"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:server": "tsup src/server/index.ts --outDir dist/server",
    "build:client": "node bundle-client.mjs && tsup src/client/index.ts --outDir dist/client",
    "build:js": "node bundle-client.mjs && tsup src/client/core/index.ts --outDir dist/client/core",
    "build:config": "tsup src/next/config/index.ts --outDir dist/next/config",
    "build:nuxt": "tsup src/nuxt/module.ts --outDir dist/nuxt",
    "build:noop": "tsup src/noop.ts --outDir dist",
    "build": "npm run build:noop && npm run build:server && npm run build:client && npm run build:config && npm run build:js && npm run build:nuxt",
    "dev": "concurrently \"tsup src/server/index.ts --outDir dist/server --watch\" \"tsup src/client/index.tsx --outDir dist/client --watch\" \"tsup src/next/config/index.ts --outDir dist/next/config --watch\" \"tsup src/client/core/index.ts --outDir dist/client/core --watch\"",
    "version-packages": "changeset version",
    "changeset": "changeset",
    "release": "pnpm build && changeset publish",
    "test": "vitest"
  }
}